<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#AC5A20" />
            <gradient android:angle="-90" android:startColor="#AC5A20" android:endColor="#AC5A20"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#AC5A20" />
            <solid android:color="#AC5A20"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#AC5A20" />
            <gradient android:angle="-90" android:startColor="#EE7B18" android:endColor="#EE7B18" />
        </shape>
    </item>
</selector>