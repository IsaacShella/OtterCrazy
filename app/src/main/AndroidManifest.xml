<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isaacss.ottercrazy" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".OtterMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OtterNumber2"
            android:label="@string/title_activity_otter_number2"
            android:parentActivityName=".OtterMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.isaacss.ottercrazy.OtterMain" />
        </activity>
        <activity
            android:name=".OtterNumber3"
            android:label="@string/title_activity_otter_number3" >
        </activity>
    </application>

</manifest>
